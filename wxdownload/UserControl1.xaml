        <Image  Name="image1" Stretch="Uniform"   MouseDown="image1_MouseDown" MouseMove="image1_MouseMove" MouseLeave="image1_MouseLeave" RenderTransformOrigin="0.5,0.5" MouseWheel="image1_MouseWheel" MouseUp="image1_MouseUp" Margin="0" />
